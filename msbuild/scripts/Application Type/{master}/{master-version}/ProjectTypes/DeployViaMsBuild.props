<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Import Project="$(MSBuildThisFileDirectory)$(MSBuildThisFileName)\*.props" Condition="'$(__DeployViaMsBuildTargetsProps)' != 'true'" />

  <PropertyGroup>

    <!--
      Launch configuration options.
    -->

    <DebuggerConfigMode Condition="'$(DebuggerConfigMode)' == ''">Custom</DebuggerConfigMode>

    <DebuggerConfigTargetApk Condition="'$(DebuggerConfigTargetApk)' == ''">$(OutDir)$(ProjectName)-$(Configuration.Replace(' ','_')).apk</DebuggerConfigTargetApk>

    <DebuggerConfigUpToDateCheck Condition="'$(DebuggerConfigUpToDateCheck)' == ''">true</DebuggerConfigUpToDateCheck>

    <DebuggerConfigCustomLaunchActivity Condition="'$(DebuggerConfigCustomLaunchActivity)' == ''"></DebuggerConfigCustomLaunchActivity>

    <DebuggerConfigDebugMode Condition="'$(DebuggerConfigDebugMode)' == ''">true</DebuggerConfigDebugMode>

    <DebuggerConfigOpenGlTrace Condition="'$(DebuggerConfigOpenGlTrace)' == ''">false</DebuggerConfigOpenGlTrace>

    <DebuggerConfigKeepAppData Condition="'$(DebuggerConfigKeepAppData)' == ''">true</DebuggerConfigKeepAppData>

    <DebuggerConfigInstallerPackage Condition="'$(DebuggerConfigInstallerPackage)' == ''"></DebuggerConfigInstallerPackage>

    <!--
      Launch properties.
    -->

    <DebuggerPropCheckJni Condition="'$(DebuggerPropCheckJni)' == ''">true</DebuggerPropCheckJni>

    <DebuggerPropEglCallstack Condition="'$(DebuggerPropEglCallstack)' == ''">true</DebuggerPropEglCallstack>

    <__DeployViaMsBuildTargetsProps>true</__DeployViaMsBuildTargetsProps>

  </PropertyGroup>

</Project>
