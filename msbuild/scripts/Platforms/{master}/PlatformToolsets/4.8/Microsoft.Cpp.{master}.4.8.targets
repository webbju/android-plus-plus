<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <!-- 
    only remote debuggers can be used for arm binaries
  -->
  
  <PropertyGroup>
    <UseDefaultDebuggersPropertyPageSchemas>false</UseDefaultDebuggersPropertyPageSchemas>
  </PropertyGroup>

  <ItemGroup>
    <AppHostDebuggerPages Include="$(VCTargetsPath)$(LangID)\AppHostDebugger_Remote.xml" />
    <DesktopDebuggerPages Include="$(VCTargetsPath)$(LangID)\debugger_remote_windows.xml" />
  </ItemGroup>

  <!--
    Pass-through support for properties within Visual Studio 2012.
  -->

  <Import Project="$(VCTargetsPath)\Platforms\{master}\{master}.Cpp.targets" />

  <Import Project="$(VCTargetsPath)\Platforms\{master}\PlatformToolsets\$(PlatformToolset)\{master}.Cpp.$(PlatformToolset).targets" />

</Project>

